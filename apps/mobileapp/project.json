{
  "name": "mobileapp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mobileapp/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project mobileapp --web",
  "targets": {
    "run-ios": {
      "executor": "@nx/react-native:run-ios",
      "options": {
        "port": 8081
      },
      "configurations": {
        "iphone15": {
          "simulator": "iPhone 15"
        },
        "iphone14": {
          "simulator": "iPhone 14"
        },
        "ipad": {
          "simulator": "iPad Pro (12.9-inch) (6th generation)"
        }
      }
    }
  }
}
